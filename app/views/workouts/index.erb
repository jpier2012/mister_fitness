<h1>Workouts page</h1>
<br>
<h2>Click a workout name to see the workout detail page</h2>
<br>
<ol>
<% @workouts.each do |w| %>
  <li><h3><a href="/workouts/<%= w.id %>"><%= w.name %></a></h3></li>
  <br>
  <ul>
  <li id="tab">Date logged: <%= w.created_at.strftime("%m-%d-%y") %></li>
  <li id="tab">Description: <%= w.description %></li>
  </ul>
  <br>
<% end %>
</ol>
<br>
<a href="/workouts/new"><input type="submit" value="Create New Workout"></a>
<br>
<br>
<a href="/workouts/community"><input type="submit" value="See Community Workouts"></a>